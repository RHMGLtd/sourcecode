<viewdata model="rhmg.StudioDiary.InternalWeb.ViewModels.EditRateModel" />

<content name="head">
  <title>Edit a Rate</title>
</content>

<content name="body">
  <div class="header">
    <h1>Edit a Rate</h1>
  </div>
  <form method="post">
    <input type="hidden" name="RateId" value="!{Model.RateId}" />
    <rate />
  </form>
  <hr />
  <existingRates rates="Model.CurrentRates" />
</content>